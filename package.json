{
  "name": "ngx-matomo",
  "version": "3.0.0",
  "author": "Arnaud Mouronval <arnaud.mouronval@gmail.com>",
  "description": "Matomo (aka. Piwik) web analytics for Angular applications.",
  "keywords": [
    "matomo",
    "piwik",
    "angular",
    "aot",
    "web analytics"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Arnaud73/ngx-matomo.git"
  },
  "bugs": {
    "url": "https://github.com/Arnaud73/ngx-matomo/issues"
  },
  "homepage": "https://github.com/Arnaud73/ngx-matomo#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo": "npm run build:ngx-matomo:prod && ng serve demo --open",
    "build": "ng build",
    "build:ngx-matomo": "ng build ngx-matomo",
    "build:ngx-matomo:prod": "ng build ngx-matomo --configuration production",
    "build:demo": "ng build demo",
    "build:demo:prod": "ng build demo --configuration production",
    "build:demo:no-tracking": "ng build demo --configuration no-tracking",
    "watch": "ng build --watch --configuration development",
    "watch:prod": "ng build --watch --configuration production",
    "test": "ng test",
    "test:ci": "ng test --watch=false --progress=false --code-coverage --browsers=Chrome",
    "lint": "ng lint",
    "clean": "rimraf dist/*",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "generate-doc": "compodoc -c ./.compodocrc.json",
    "copy-license": "copyfiles ./LICENSE ./dist/ngx-matomo",
    "copy-readme": "copyfiles ./README.md ./dist/ngx-matomo",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@compodoc/compodoc": "^1.1.25",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "18.0.1",
    "copyfiles": "^2.4.0",
    "eslint": "^9.3.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.4.2",
    "typescript-eslint": "8.0.0-alpha.20"
  },
  "standard-version": {}
}
